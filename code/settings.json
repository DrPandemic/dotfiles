{
    "workbench.colorTheme": "Noctis Minimus",
    "files.insertFinalNewline": true,
    "files.trimTrailingWhitespace": true,
    "window.zoomLevel": 1,
    "editor.lineNumbers": "relative",
    "editor.minimap.enabled": false,
    "editor.rulers": [
        80,
        120
    ],
    "workbench.iconTheme": "material-icon-theme",
    "workbench.sideBar.location": "right",
    "material-icon-theme.showUpdateMessage": false,
    "rust.rustup": {
        "toolchain": "stable-x86_64-unknown-linux-gnu",
        "nightlyToolchain": "nightly-x86_64-unknown-linux-gnu"
    },
    "rust.mode": "legacy",
    "GodotTools.editorPath": "/opt/godot-mono-bin/godot/Godot_v3.1.1-stable_mono_x11.64",
    "keyboard.dispatch": "keyCode",
    "workbench.editorAssociations": {
        "*.ipynb": "jupyter.notebook.ipynb"
    },
    "vim.easymotion": true,
    "vim.useSystemClipboard": true,
    "vim.normalModeKeyBindingsNonRecursive": [
        {
            "before": [
                "<space>"
            ],
            "commands": [
                "vspacecode.space"
            ]
        },
        {
            "before": [
                ","
            ],
            "commands": [
                "vspacecode.space",
                {
                    "command": "whichkey.triggerKey",
                    "args": "m"
                }
            ]
        }
    ],
    "vim.visualModeKeyBindingsNonRecursive": [
        {
            "before": [
                "<space>"
            ],
            "commands": [
                "vspacecode.space"
            ]
        },
        {
            "before": [
                ","
            ],
            "commands": [
                "vspacecode.space",
                {
                    "command": "whichkey.triggerKey",
                    "args": "m"
                }
            ]
        }
    ],
    "window.titleBarStyle": "custom",
    "projectManager.git.baseFolders": [
        "~/Projects"
    ],
    "terminal.integrated.rendererType": "experimentalWebgl",
    "trailing-spaces.highlightCurrentLine": false,
    "trailing-spaces.deleteModifiedLinesOnly": true,
    "workbench.statusBar.visible": true,
    "whichkey.bindingOverrides": [
        {
            "keys": ["m", "languageId:ruby", "t", "t"],
            "name": "Test under cursor",
            "type": "command",
            "command": "runTestLine",
        }
    ],
    "editor.autoClosingOvertype": "never",
    "editor.autoClosingQuotes": "never",
    "editor.autoSurround": "never",
    "editor.autoClosingBrackets": "never",
    "workbench.editor.enablePreviewFromQuickOpen": false,
    "vim.enableNeovim": true,
    "vim.highlightedyank.enable": true,
    "vim.statusBarColorControl": true,
    "workbench.colorCustomizations": {
        "statusBar.background": "#005f5f",
        "statusBar.noFolderBackground": "#005f5f",
        "statusBar.debuggingBackground": "#005f5f",
        "statusBar.foreground": "#a8dada"
    },
    "workbench.activityBar.visible": false,
    "omnisharp.monoPath": "/usr/lib/mono/msbuild/Current",
    "omnisharp.useGlobalMono": "always",
    "omnisharp.enableRoslynAnalyzers": true,
    "omnisharp.organizeImportsOnFormat": true,
    "editor.acceptSuggestionOnCommitCharacter": false,
    "vim.handleKeys": {
        "<C-n>": false,
        "<C-d>": true,
        "<C-s>": false,
        "<C-z>": false
    },
    "extensions.ignoreRecommendations": true,
}
