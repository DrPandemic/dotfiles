{
    "workbench.colorTheme": "Noctis Minimus",
    "files.insertFinalNewline": true,
    "files.trimTrailingWhitespace": true,
    "editor.lineNumbers": "relative",
    "editor.minimap.enabled": false,
    "editor.rulers": [
        80,
        120
    ],
    "workbench.iconTheme": "material-icon-theme",
    "workbench.sideBar.location": "right",
    "material-icon-theme.showUpdateMessage": false,
    "rust.rustup": {
        "toolchain": "stable-x86_64-unknown-linux-gnu",
        "nightlyToolchain": "nightly-x86_64-unknown-linux-gnu"
    },
    "rust.mode": "rls",
    "GodotTools.editorPath": "/opt/godot-mono-bin/godot/Godot_v3.1.1-stable_mono_x11.64",
    "keyboard.dispatch": "keyCode",
    "workbench.editorAssociations": {
        "*.ipynb": "jupyter.notebook.ipynb"
    },
    "vim.easymotion": true,
    "vim.useSystemClipboard": true,
    "vim.normalModeKeyBindingsNonRecursive": [
        {
            "before": [
                "<space>"
            ],
            "commands": [
                "vspacecode.space"
            ]
        },
        {
            "before": [
                ","
            ],
            "commands": [
                "vspacecode.space",
                {
                    "command": "whichkey.triggerKey",
                    "args": "m"
                }
            ]
        },
        {
            "before": [
                "Y"
            ],
            "after": [
                "y",
                "$"
            ]
        }
    ],
    "vim.visualModeKeyBindingsNonRecursive": [
        {
            "before": [
                "<space>"
            ],
            "commands": [
                "vspacecode.space"
            ]
        },
        {
            "before": [
                ","
            ],
            "commands": [
                "vspacecode.space",
                {
                    "command": "whichkey.triggerKey",
                    "args": "m"
                }
            ]
        }
    ],
    "window.titleBarStyle": "custom",
    "projectManager.git.baseFolders": [
        "~/Projects"
    ],
    "terminal.integrated.rendererType": "experimentalWebgl",
    "trailing-spaces.highlightCurrentLine": false,
    "trailing-spaces.deleteModifiedLinesOnly": true,
    "workbench.statusBar.visible": true,
    "whichkey.bindingOverrides": [
        {
            "keys": ["m", "languageId:ruby", "t", "t"],
            "name": "Test under cursor",
            "type": "command",
            "command": "runTestLine",
        }
    ],
    "editor.autoClosingOvertype": "never",
    "editor.autoClosingQuotes": "never",
    "editor.autoSurround": "never",
    "editor.autoClosingBrackets": "never",
    "workbench.editor.enablePreviewFromQuickOpen": false,
    "vim.enableNeovim": true,
    "vim.highlightedyank.enable": true,
    "vim.statusBarColorControl": true,
    "workbench.colorCustomizations": {
        "statusBar.background": "#005f5f",
        "statusBar.noFolderBackground": "#005f5f",
        "statusBar.debuggingBackground": "#005f5f",
        "statusBar.foreground": "#ffffff",
        "statusBar.debuggingForeground": "#ffffff"
    },
    "omnisharp.monoPath": "/usr/lib/mono/msbuild/Current",
    "omnisharp.useGlobalMono": "always",
    "omnisharp.enableRoslynAnalyzers": true,
    "omnisharp.organizeImportsOnFormat": true,
    "editor.acceptSuggestionOnCommitCharacter": false,
    "vim.handleKeys": {
        "<C-n>": false,
        "<C-d>": true,
        "<C-s>": false,
        "<C-z>": false
    },
    "extensions.ignoreRecommendations": true,
    "rust-analyzer.inlayHints.chainingHints.enable": false,
    "rust-analyzer.inlayHints.closingBraceHints.enable": false,
    "rust-analyzer.inlayHints.typeHints.enable": false,
    "[json]": {
        "editor.defaultFormatter": "vscode.json-language-features"
    },
    "editor.tabSize": 2,
    "[javascript]": {
        "editor.defaultFormatter": "vscode.typescript-language-features"
    },
    "rust-analyzer.inlayHints.parameterHints.enable": false,
    "rust-analyzer.check.command": "clippy",
    "redhat.telemetry.enabled": false,
    "rust.rls": {
        "useRustfmt": true
    },
    "workbench.activityBar.location": "hidden",
    "gitlens.telemetry.enabled": false,
    "telemetry.telemetryLevel": "off",
    "editor.acceptSuggestionOnEnter": "off",
    "editor.snippets.codeActions.enabled": false,
    "editor.suggest.showSnippets": false,
    "editor.fontFamily": "'Inconsolata Nerd Font Mono', 'Twemoji', 'Droid Sans Mono', 'monospace', monospace",
    "dev.containers.dockerSocketPath": "/var/run/docker.sockdocker",
    "zig.zls.enabled": "on",
    "zig.zls.inlayHintsShowParameterName": false,
    "zig.zls.inlayHintsShowVariableTypeHints": false,
    "[zig]": {
        "editor.formatOnSave": true,
        "editor.defaultFormatter": "ziglang.vscode-zig",
        "editor.stickyScroll.defaultModel": "foldingProviderModel",
        "editor.tabSize": 4,
        "files.eol": "\n"
    },
    "github.copilot.enable": {
        "*": false,
        "plaintext": false,
        "markdown": false,
        "scminput": false
    },
    "zig.path": "zig",
    "gitlens.currentLine.enabled": false,
}
